#include <stdio.h>

int main() {
    int n, i;

    
    printf("Enter a positive integer n: ");
    scanf("%d", &n);

    
    if (n <= 0) {
        printf("Please enter a number greater than 0.\n");
        return 1;
    }

    
    for (i = 1; i <= n; i++) {
        printf("%d ", i);
    }

    printf("\n");  // Newline after printing all numbers
    return 0;
}
