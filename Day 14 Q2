#include <stdio.h>

int main() {
    int n, i;
    unsigned long long product = 1;  // Use unsigned long long for larger product values
    int evenFound = 0;  // Flag to check if any even number found

   
    printf("Enter a positive integer n: ");
    scanf("%d", &n);

    if (n < 2) {
        printf("No even numbers between 1 and %d\n", n);
        return 0;
    }

    
    for (i = 2; i <= n; i += 2) {
        product *= i;
        evenFound = 1;
    }

    if (evenFound) {
        printf("Product of even numbers from 1 to %d is: %llu\n", n, product);
    } else {
        printf("No even numbers found to multiply.\n");
    }

    return 0;
}
